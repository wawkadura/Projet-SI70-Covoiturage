{% extends 'base.html.twig' %}

{% block body %}
<br>
<div class="jumbotron">
<h6 class="display-4">Inscription</h6>
{{form_start(formUtilisateur)}}
  <fieldset>
    <div class="form-group">
      <label>Nom</label>
      {{form_widget(formUtilisateur.nom)}}
      {# <input type="text" class="form-control" required name="nom" placeholder="Entrée votre nom"> #}
    </div>
    <div class="form-group">
      <label>Prénom</label>
      {{form_widget(formUtilisateur.prenom)}}
      {# <input type="text" class="form-control" required name="prenom" placeholder="Entrée votre prénom"> #}
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Date de naissance : </label>
      {{form_widget(formUtilisateur.datedenaissance)}}
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Téléphone : </label>
      {{form_widget(formUtilisateur.telephone)}}
      {# <input type="text" class="form-control" id="exampleInputPassword1" required name="telephone" placeholder="Entrée votre numéro de téléphone (format 0612345678)" > #}
    </div>
  </fieldset>
    {{form_end(formUtilisateur)}}
  <form method="post" action="">
    <div class="form-group">
      <label for="exampleInputEmail1">Adress Mail : </label>
      <input type="email" class="form-control" id="exampleInputEmail1" required  name="email" aria-describedby="emailHelp" placeholder="Entrée votre email">
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Mot de passe : </label>
      <input type="password" class="form-control" id="exampleInputPassword1" required name="password" placeholder="Mot de passe">
    </div>
    <div class="form-group">
      <label for="exampleInputPassword2">Confirmé mot de passe : </label>
      <input type="password" class="form-control" id="exampleInputPassword2" name="password1" placeholder="Mot de passe">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
  
</form>
</div>

{% endblock %}